se=>{\r\n                    if(response.payload.success){\r\n                        setIsFollow(response.payload.result);\r\n                        dispatch(response);\r\n                    }\r\n                }\r\n            );\r\n        }\r\n    }, [_key]);\r\n\r\n    // 팔로우 on / off\r\n    const onIsFollowHandler = () => {\r\n        setIsFollow(!isFollow);\r\n        const body = {\r\n            follow: !isFollow,\r\n            key : _key,\r\n        };\r\n        followUser(body).then(\r\n            response => {\r\n                console.log(response);\r\n        });\r\n    };\r\n    return (\r\n    <>\r\n        <GlobalStyle />\r\n        <Whole>\r\n            {/* 작가 아이콘, 네임 */}\r\n            <IDBox>\r\n                <a href={`${CLIENT_PATH}personal/${_key}`}>\r\n                    <PersonalImg src={!personalImgPath ? defaultImg : `${SERVER_PATH}${personalImgPath}`}/>\r\n                </a>\r\n                <a href={`${CLIENT_PATH}personal/${_key}`}>\r\n                    <p>{authorName}</p>\r\n                </a>\r\n                <PhotoBlueBtn onClick={onIsFollowHandler} >{isFollow ? `Followed!` : `Follow!`}</PhotoBlueBtn>\r\n            </IDBox>\r\n            {/* 작가 작품 리스트 */}\r\n            <ImgList>\r\n                {photo && photo.map((tmp:IPhoto, index:number)=>(\r\n                    <a key={index} href={`${CLIENT_PATH}photo/${tmp.id}`}><ImgBox src={`${SERVER_PATH}${tmp.path}`}/></a>))}\r\n            </ImgList>\r\n        </Whole>\r\n    </>);\r\n}\r\nexport default AuthorImgList;","C:\\Users\\yoon\\Desktop\\STUDY\\17_FE\\1_Project\\6_newone\\client\\src\\page\\photo\\material\\Comment.tsx",["255","256"],"C:\\Users\\yoon\\Desktop\\STUDY\\17_FE\\1_Project\\6_newone\\client\\src\\page\\login\\Login.tsx",["257","258"],"import React, { useState } from 'react';\r\nimport styled, {css} from 'styled-components';\r\nimport GlobalStyle from 'globalStyles';\r\nimport {InputBox} from 'component/input';\r\nimport {BlueBtn} from 'compone